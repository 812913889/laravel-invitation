{
  "name": "lampge/laravel-invitation",
  "description": "Laravel邀请码组件",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "lampge",
      "email": "lampge@sina.com"
    }
  ],
  "require": {
    "php": ">=7.1.2",
    "illuminate/database": "~5.6",
    "illuminate/support": "~5.6",
    "ariby/ulid": "^0.2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.1.4",
    "orchestra/testbench": "~3.6",
    "Orchestra/Database": "~3.6",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Ariby\\LaravelInvitation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ariby\\tests\\": "tests/"
    },
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Ariby\\LaravelInvitation\\LaravelInvitationServiceProvider"
      ],
      "aliases": {
        "LaravelInvitation": "Ariby\\LaravelInvitation\\Facades\\LaravelInvitation"
      }
    }
  }
}
